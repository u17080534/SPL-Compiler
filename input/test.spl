hello;

proc guess
{
    while(not eqv)
    {
        num answer;
        num other;
        other = 7;

        input (answer);

        eqv = eq(answer, other);

        if(eqv) then
        {
            output (correct)
        }
        else
        {
            output (wrong)
        }
    }
}

proc hello
{
    string h;
    h = "hello";
    hello2;
    guess;

    proc hello2
    {
